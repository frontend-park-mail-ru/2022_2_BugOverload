{
    "extends":["airbnb/base"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "eol-last": ["error", "always"],
        "semi": ["error", "always"],
        "no-console": 1,
        "no-alert": 2,
        "no-debugger": 1,
        "no-use-before-define": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0
    }
}
